{
    "version": 2,
    "functions": {
        "api/index.js": {
            "runtime": "@vercel/node"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/assets/(.*)",
            "destination": "/frontend/dist/assets/$1"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/frontend/dist/index.html"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}